    "simple-git-hooks": "^2.13.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpx lint-staged"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "^10"
  },
  "packageManager": "pnpm@10.30.0",
  "pnpm": {
